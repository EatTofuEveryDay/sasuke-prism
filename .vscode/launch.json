{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node.JS Debug",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/src/index.js",
      "runtimeArgs": ["--unhandled-rejections=strict"],
      "cwd": "${workspaceFolder}/bin/src"
    }
  ]
}